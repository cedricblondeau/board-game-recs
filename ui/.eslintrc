{
  "plugins": [
    "react",
    "prettier"
  ],
  "extends": [
    "react-app",
    "airbnb",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "react/destructuring-assignment": [
      "error",
      "never"
    ],
    "no-console": "off",
    "func-names": "off",
    "jsx-a11y/label-has-associated-control": [
      "error",
      {
        "required": {
          "some": [
            "nesting",
            "id"
          ]
        }
      }
    ],
    "react/jsx-filename-extension": [
      "warn",
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}
